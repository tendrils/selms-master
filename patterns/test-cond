[global] {
}

[host test ] {
  [periodic] {
    proc < 'xxxx'			: drop 
    proc <= 'xxxx'			: drop 
    proc != 'xxxx'			: drop 
    proc    'xxxx'			: drop 
    proc > 'xxxx'			: drop 
    proc >= 'xxxx'			: drop 
    proc ( 'xxxx' .. 'zzz' )		: drop 

    int < 10			: drop 
    int <= 10			: drop 
    int != 10			: drop 
    int    10			: drop 
    int > 10			: drop 
    int >= 10			: drop 
    int (10..20)		: drop 

    /xxxx(\d+)/		: count 10 "yyyy %1", drop
    /bad/		: count 10 "bad", drop
    /aaaa/		: proc test


   }


}

